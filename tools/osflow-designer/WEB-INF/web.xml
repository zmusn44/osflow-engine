<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>


  <filter>
    <filter-name>CommonFilter</filter-name>
    <filter-class>cn.linkey.filter.CommonFilter</filter-class>
  </filter>

  <filter-mapping>
    <filter-name>CommonFilter</filter-name>
    <url-pattern>/rule</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>CommonFilter</filter-name>
    <url-pattern>/linkey/bpm/newFlow/view/rule</url-pattern>
  </filter-mapping>


  <welcome-file-list>
    <welcome-file>/ProcessGrid.html</welcome-file>
  </welcome-file-list>

</web-app>


